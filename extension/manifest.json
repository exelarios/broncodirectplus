{
  "manifest_version": 3,
  "name": "BroncoDirect+",
  "description": "Displays instructors' ratings directly into BroncoDirect",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://cmsweb.cms.cpp.edu/*"],
      "js": ["./index.js"],
      "all_frames": true
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "BroncoDirect+"
  }
}